<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.tour_tour">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_image"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Churches"
            android:label="CHURCHES" />
        <activity
            android:name=".Malls"
            android:label="MALLS" />
        <activity
            android:name=".Restaurants"
            android:label="RESTAURANTS" />
        <activity
            android:name=".Schools"
            android:label="SCHOOLS" />
        <activity android:name=".DetailsActivity"></activity>
    </application>

</manifest>